{
  "name": "mailscript",
  "description": "Configure and manipulate email pipeline in Mailscript from the cli",
  "version": "0.3.13",
  "author": "team <team@mailscript.com>",
  "bin": {
    "mailscript": "./bin/run"
  },
  "bugs": "https://github.com/getmailscript/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "body-parser": "^1.19.0",
    "cli-ux": "^5.5.1",
    "deep-equal": "2.0.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "jose": "^2.0.3",
    "js-yaml": "^3.14.0",
    "node-fetch": "^2.6.1",
    "oazapfts": "3.1.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/deep-equal": "1.0.1",
    "@types/express": "^4.17.9",
    "@types/js-yaml": "3.12.5",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.7",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.15.0",
    "globby": "^10",
    "mocha": "^5",
    "nock": "13.0.5",
    "nyc": "^14",
    "prettier": "^2.1.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/www",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/getmailscript/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mailscript",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "getmailscript/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint ./src --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "MAILSCRIPT_CONFIG_PATH=./test/test-mailscript-config.json nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\"",
    "swagger:client-compile": "./scripts/compile-client.sh"
  },
  "types": "lib/index.d.ts"
}
